{
  "name": "valpas",
  "version": "0.0.1",
  "scripts": {
    "start": "parcel src/index.html",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest src",
    "test:integration": "npm run build:local && jest --config jest.integrationtests.config.js test/integrationtests",
    "test:integration:debug": "SHOW_BROWSER=true npm run test:integration",
    "build:local": "NODE_ENV=development parcel build src/index.html",
    "build:prod": "parcel build src/index.html",
    "lint": "npm run lint:types && npm run lint:format",
    "lint:types": "tsc --noEmit",
    "lint:format": "prettier --check ./src",
    "fix": "npm run fix:format",
    "fix:format": "prettier --write ./src",
    "clean": "rm -rf dist && rm -rf .cache",
    "preinstall": "npx npm-force-resolutions"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Opetushallitus/koski.git"
  },
  "license": "EUPL-1.1",
  "homepage": "https://github.com/Opetushallitus/koski#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@types/jest": "^26.0.20",
    "@types/ramda": "^0.27.34",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@types/selenium-webdriver": "^4.0.11",
    "babel-jest": "^26.6.3",
    "chromedriver": "^87.0.5",
    "jest": "^26.6.3",
    "less": "^4.0.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.2.1",
    "react-test-renderer": "^17.0.1",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "bem-ts": "^0.11.7",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  }
}