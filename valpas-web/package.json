{
  "name": "valpas",
  "version": "0.0.1",
  "description": "Valpas",
  "license": "EUPL-1.1",
  "engines": {
    "node": "14",
    "npm": "7"
  },
  "scripts": {
    "start": "node autoclear.js && parcel serve src/index.html",
    "start:watch": "concurrently npm:start npm:lint:types:watch",
    "start:raamit": "node autoclear.js --dev-raamit && VIRKAILIJA_RAAMIT_HOST=\"https://virkailija.untuvaopintopolku.fi/\" OPPIJA_RAAMIT_HOST=\"https://untuvaopintopolku.fi/\" parcel serve src/index.html",
    "start:local-oppijaraamit": "node autoclear.js --dev-local-oppijaraamit && OPPIJA_RAAMIT_HOST=\"http://localhost:8080/\" parcel serve src/index.html",
    "start:integration": "node autoclear.js --integration && parcel serve src/index.html --no-hmr --no-cache",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "node autoclear.js --unittest && jest --runInBand src",
    "test:integration": "jest --config jest.integrationtests.config.js --runInBand test/integrationtests",
    "test:integration:debug": "SHOW_BROWSER=true jest --config jest.integrationtests.config.js --runInBand test/integrationtests",
    "test:integration:browserstack": "jest --config jest.integrationtests.browserstack.config.js --runInBand test/integrationtests",
    "build:local": "node autoclear.js && NODE_ENV=development parcel build src/index.html --no-scope-hoist",
    "build:prod": "node autoclear.js --prod && parcel build src/index.html --public-url /valpas --no-scope-hoist",
    "lint": "npm run lint:types && npm run lint:format && npm run lint:problems",
    "lint:types": "tsc --noEmit",
    "lint:types:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "lint:format": "prettier --check ./src",
    "lint:problems": "eslint . --ext .ts --ext .tsx",
    "fix": "npm run fix:format",
    "fix:format": "prettier --write ./src",
    "clean": "jest --clearCache && rm -rf dist && rm -rf .parcel-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Opetushallitus/koski.git"
  },
  "homepage": "https://github.com/Opetushallitus/koski#readme",
  "devDependencies": {
    "@jest-environment-browserstack/selenium-webdriver": "^2.0.1",
    "@parcel/babel-plugin-transform-runtime": "^2.2.1",
    "@parcel/transformer-less": "^2.2.1",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/downloadjs": "^1.4.3",
    "@types/jest": "^26.0.20",
    "@types/jest-expect-message": "^1.0.3",
    "@types/js-cookie": "^2.2.6",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^17.0.0",
    "@types/selenium-webdriver": "^4.0.16",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "chromedriver": "^98.0.1",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.17.2",
    "http-proxy-middleware": "^1.0.6",
    "jest": "^26.6.3",
    "jest-environment-browserstack": "^2.0.1",
    "jest-expect-message": "^1.0.2",
    "jest-fail-on-console": "^2.1.1",
    "jest-fetch-mock": "^3.0.3",
    "js-cookie": "^2.2.1",
    "less": "^4.1.2",
    "node-fetch": "^2.6.7",
    "parcel": "^2.0.1",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "selenium-webdriver": "^4.1.1",
    "tree-kill": "^1.2.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "bem-ts": "^0.11.7",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.28.0",
    "downloadjs": "^1.4.7",
    "fp-ts": "^2.11.8",
    "react": "^17.0.1",
    "react-date-picker": "^8.3.6",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  }
}
