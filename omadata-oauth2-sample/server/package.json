{
  "name": "omadata-oauth2-sample",
  "version": "0.0.2",
  "description": "Sample web application to use KOSKI OmaData with OAuth2",
  "main": "index.ts",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "tsx watch --clear-screen=false src/index.ts",
    "start:prod": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "pnpm run lint:types && pnpm run lint:problems",
    "lint:types": "tsc --noEmit",
    "lint:types:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "lint:problems": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "fix": "pnpm run fix:format",
    "fix:format": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\" --fix",
    "preinstall": "node -e \"if(!process.env.npm_execpath.includes(\\\"pnpm\\\")){console.error(\\\"‚ùå Please use pnpm\\\");process.exit(1)}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Opetushallitus/koski.git"
  },
  "license": "EUPL-1.1",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.950.0",
    "body-parser": "^2.2.1",
    "express": "^5.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "openid-client": "^6.8.1",
    "undici": "^7.16.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.936.0",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.2.0+sha512.0d27364e0139c6aadeed65ada153135e0ca96c8da42123bd50047f961339dc7a758fc2e944b428f52be570d1bd3372455c1c65fa2e7aa0bfbf931190f9552001"
}
