{
  "name": "omadata-oauth2-sample",
  "version": "0.0.2",
  "description": "Sample web application to use KOSKI OmaData with OAuth2",
  "main": "index.ts",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "../scripts/start-node-sample.sh",
    "start:prod": "node dist/index.js",
    "start:alb-proxy": "../scripts/start-alb-proxy.sh",
    "start:node-sample": "../scripts/start-node-sample.sh",
    "start:java-sample": "../scripts/start-java-sample.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "pnpm -C ../../smoketests run omadata:local",
    "playwright:test": "pnpm exec playwright test",
    "lint": "pnpm run lint:types && pnpm run lint:problems",
    "lint:types": "tsc --noEmit",
    "lint:types:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "lint:problems": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "fix": "pnpm run fix:format",
    "fix:format": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\" --fix",
    "preinstall": "node -e \"if(!process.env.npm_execpath.includes(\\\"pnpm\\\")){console.error(\\\"‚ùå Please use pnpm\\\");process.exit(1)}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Opetushallitus/koski.git"
  },
  "license": "EUPL-1.1",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.974.0",
    "body-parser": "^2.2.2",
    "express": "^5.2.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "openid-client": "^6.8.1",
    "undici": "^7.18.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.969.0",
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "brace-expansion@^1": "1.1.12",
      "brace-expansion@^2": "2.0.2"
    }
  },
  "packageManager": "pnpm@10.2.0"
}
