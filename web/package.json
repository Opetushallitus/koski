  "scripts": {
    "lint": "eslint \"app/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"app/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:tests": "eslint \"test/**/*.{js,jsx,ts,tsx}\"",
    "lint:tests:fix": "eslint \"test/**/*.{js,jsx,ts,tsx}\" --fix",
    "build:prod": "webpack --bail --mode production",
    "watch": "webpack --devtool inline-source-map --progress --watch --mode development",
    "unit-test": "mocha --require ts-node/register --extension js,ts --reporter spec ./test/unit-test",
    "unit-test-watch": "mocha -w --require ts-node/register --extension js,ts --reporter spec ./test/unit-test",
    "test": "node test/mocha-chrome-wrapper.js ${MOCHA_URL:=http://localhost:7021/koski/test/runner.html}?grep=${MOCHA_GREP} --chrome-flags '[\"--use-mock-keychain\"]'",
    "playwright:test": "npx playwright test",
    "playwright:test:debug": "npx playwright test --headed",
    "playwright:test:trace": "npx playwright test --trace on && npx playwright show-report",
    "playwright:codegen": "npx playwright codegen",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "prettier:check": "prettier --check app",
    "prettier:fix": "prettier --write app",
    "prettier:tests:fix": "prettier --write test"
  }
